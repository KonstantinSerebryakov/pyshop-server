{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/prisma-client-library/(.*)": "<rootDir>/../libs/prisma-client-library/src/$1",
    "@app/prisma-client-library": "<rootDir>/../libs/prisma-client-library/src",
    "@app/interfaces/(.*)": "<rootDir>/../libs/interfaces/src/$1",
    "@app/interfaces": "<rootDir>/../libs/interfaces/src",
    "@app/decorators/(.*)": "<rootDir>/../libs/decorators/src/$1",
    "@app/decorators": "<rootDir>/../libs/decorators/src"
  }
}